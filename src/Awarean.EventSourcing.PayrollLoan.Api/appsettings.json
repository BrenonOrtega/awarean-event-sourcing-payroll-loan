{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "EventStoreOptions": {
    "CommitEventQuery": {
      "Query": "INSERT INTO @TYPE VALUES(@ENTITY_ID, @CREATED_AT, @VERSION, @SERIALIZED_EVENT);",
      "Parameters": {
        "Table": "@TABLE",
        "EntityId": "@ENTITY_ID",
        "CreatedAt": "@CREATED_AT",
        "Type": "@TYPE",
        "Version": "@VERSION",
        "SerializedEvent": "@SERIALIZED_EVENT"
      }
    },
    "GetEventsQuery": {
      "Query": "SELECT * FROM @TYPE VALUES(@ENTITY_ID, @CREATED_AT, @VERSION, @SERIALIZED_EVENT);",
      "Parameters": {
        "Table": "@TABLE",
        "EntityId": "@ENTITY_ID",
        "CreatedAt": "@CREATED_AT",
        "Type": "@TYPE",
        "Version": "@VERSION",
        "SerializedEvent": "@SERIALIZED_EVENT"
      }
    }
  }
}
